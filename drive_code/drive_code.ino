 Adafruit_MotorShield AFMS = Adafruit_MotorShield();
 Adafruit_DCMotor *motorFRight = AFMS.getMotor(1);
 Adafruit_DCMotor *motorFLeft = AFMS.getMotor(2);
 Adafruit_DCMotor *motorBRight = AFMS.getMotor(3);
 Adafruit_DCMotor *motorBLeft = AFMS.getMotor(4);
 Servo myservo;  //neck
int pos=0 ;    



void setup() {
  myservo.attach(10);
  myservo.write(initpos);
  pinMode(forward, INPUT);    // sets the digital pin 7 as input
  pinMode(backward, INPUT);
  pinMode(left, INPUT);
  pinMode(right, INPUT);
    AFMS.begin();
  motorFRight->setSpeed(255);
  motorFLeft->setSpeed(255);
  motorBRight->setSpeed(255);
  motorBLeft->setSpeed(255);
  myservo.attach(10);

  Serial.begin(9600);
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
  }
  display.clearDisplay();
  display_BOOT();
  eyeMoves();
  

}

void loop() {
  blink_eye();
  val1 = digitalRead(forward);   // read the input pin
  val2 = digitalRead(backward);
  val3 = digitalRead(left);
  val4 = digitalRead(right);
  
  if ((val1 == 1) && (val2 == 0) && (val3 == 0) && (val4 == 0)) { //checks if control points forward.
      motorFRight->run(FORWARD);
      motorFLeft->run(FORWARD);
      motorBRight->run(FORWARD);
      motorBLeft->run(FORWARD);
      
      
  } else if ((val1 == 0) && (val2 == 1) && (val3 == 0) && (val4 == 0)) { //checks if control points backward.
      motorFRight->run(BACKWARD);
      motorFLeft->run(BACKWARD);
      motorBRight->run(BACKWARD);
      motorBLeft->run(BACKWARD);
  } else if ((val1 == 0) && (val2 == 0) && (val3 == 1) && (val4 == 0)) { //checks if control points left.
      motorFRight->run(FORWARD);
      motorFLeft->run(BACKWARD);
      motorBRight->run(BACKWARD);
      motorBLeft->run(FORWARD);
  } else if ((val1 == 0) && (val2 == 0) && (val3 == 0) && (val4 == 1)) { //checks if control points right.
      motorFRight->run(BACKWARD);
      motorFLeft->run(FORWARD);
      motorBRight->run(FORWARD);
      motorBLeft->run(BACKWARD);
  } else {
      motorFRight->run(RELEASE);
      motorFLeft->run(RELEASE);
      motorBRight->run(RELEASE);
      motorBLeft->run(RELEASE);
      lookleft();
      for (pos = 0; pos <=psmax; pos += 10){
        myservo.write(pos);
      }
      myservo.write(initpos);
      lookright();
      for (pos = initpos; pos <=psmax; pos -= 10){
        myservo.write(pos);
      }
      eyeMoves();
      myservo.write(initpos);
  }

}
